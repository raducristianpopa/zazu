{
	"name": "zazu",
	"displayName": "zazu",
	"description": "zazu",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.83.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "zazu.helloWorld",
				"title": "Hello World"
			}
		]
	},
	"scripts": {
		"watch": "tsx ./scripts/build.ts",
		"build": "tsx ./scripts/build.ts",
		"vscode:prepublish": "npm run build",
		"compile": "webpack",
		"dev": "npm run -S esbuild-base -- --sourcemap --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"lint": "eslint src --ext ts"
	},
	"devDependencies": {
		"@tsconfig/svelte": "^5.0.2",
		"@types/node": "18.x",
		"@types/vscode": "^1.83.0",
		"@typescript-eslint/eslint-plugin": "^6.7.3",
		"@typescript-eslint/parser": "^6.7.3",
		"esbuild": "^0.19.4",
		"eslint": "^8.50.0",
		"svelte": "^4.2.1",
		"svelte-check": "^3.5.2",
		"svelte-preprocess": "^5.0.4",
		"tslib": "^2.6.2",
		"tsx": "^3.13.0",
		"typescript": "^5.2.2"
	},
	"dependencies": {
		"@interledger/open-payments": "^3.2.0"
	}
}
